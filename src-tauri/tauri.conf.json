{
  "productName": "SandCat",
  "version": "0.1.0",
  "identifier": "com.tauri.dev",
  "build": {
    "beforeDevCommand": "trunk serve",
    "beforeBuildCommand": "trunk build",
    "devUrl": "http://localhost:8080",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "SandCat",
        "width": 1280,
        "height": 720,
        "resizable": false,
        "fullscreen": false,
        "center": true,
        "focus": true,
        "closable": true,
        "decorations": true,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.svg",
      "icons/icon2.png"
    ]
  }
}
